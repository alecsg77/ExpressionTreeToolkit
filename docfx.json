{
  "metadata": [{
    "src": [{
      "files": ["**.csproj"],
      "exclude": ["**/bin/**", "**/obj/**"],
      "src": "src"
    }],
    "dest": "docfx/api",
    "disableGitFeatures": false,
    "disableDefaultFilter": false,
    "properties": {
      "TargetFramework": "netstandard2.0"
    }
  }],
  "build": {
    "content": [{
        "files": [
          "api/**.yml",
          "api/index.md"
        ],
        "src": "docfx"
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "src": "docfx"
      }
    ],
    "resource": [{
      "files": [
        "images/**"
      ],
      "src": "docfx"
    }],
    "overwrite": [{
      "files": [
        "apidoc/**.md"
      ],
      "exclude": [
        "obj/**"
      ],
      "src": "docfx"
    }],
    "dest": "gh-pages",
    "globalMetadata": {
      "_appTitle": "Expression Tree Toolkit Documentation",
      "_enableSearch": true
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "statictoc"
    ],
    "sitemap": {
      "baseUrl": "https://dotnet.github.io/docfx",
      "priority": 0.1,
      "changefreq": "monthly",
      "fileOptions": {
        "**/api/**.yml": {
          "priority": 0.3,
          "lastmod": "2001-01-01"
        }
      }
    },
    "intermediateFolder": "docfx/obj",
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "xrefService": ["https://xref.docs.microsoft.com/query?uid={uid}"]
  }
}
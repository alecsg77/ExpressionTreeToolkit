{
  "metadata": [
    {
      "src": [
        {
          "files": [ "**.csproj" ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "src": "../src/"
        }
      ],
      "dest": "obj/docfx/lib",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "properties": {
        "TargetFramework": "netstandard2.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.yml"
        ],
        "src": "obj/docfx"
      },
      {
        "files": [
          "docs/**/*.{md,yml}",
          "articles/**/*.{md,yml}",
          "*.{md,yml}"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/images/**",
          "**/artifacts/**",
          "codesnippet/**",
          "langwordmapping/**",
          "ExpressionTreeToolkit.svg",
          "ExpressionTreeToolkit.ico"
        ],
        "exclude": [ "wwwroot/**", "**/obj/**", "**.meta" ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "libdoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "Expression Tree Toolkit Documentation",
      "_appFooter": "<span>Copyright Â© alecsg77.<br>Generated by <strong>DocFX</strong></span>",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_gitContribute": {
        "branch": "dev"
      }
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/custom"
    ],
    "sitemap": {
      "baseUrl": "https://alecsg77.github.io/ExpressionTreeToolkit",
      "priority": 0.1,
      "changefreq": "monthly",
      "fileOptions": {
        "**/obj/docfx/**.yml": {
          "priority": 0.5
        }
      }
    },
    "postProcessors": [ "ExtractSearchIndex" ],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ]
  }
}
